@model List<AdoNetProject.Models.EmployeeModel>
@{
    ViewBag.Title = "Index";
    Layout ="~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<table class="table table-condensed">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
            <td>
            @Html.ActionLink("Edit","Edit",new {id=@item.EmpId })<br />
            @Html.ActionLink("Delete","Delete", new { id = @item.EmpId })
            </td>
        </tr>
    }
</table>